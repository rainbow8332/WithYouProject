<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sample.withyou_login">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.WithYou_login">
        <activity android:name=".Chating.Chat_Room"></activity>
        <activity android:name=".My.MemberInitActivity" />
        <activity android:name=".My.PasswordResetActivity" />
        <activity android:name=".My.LoginActivity" />
        <activity android:name=".Chating.ChatActivity" />
        <activity
            android:name=".My.SignUpActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name=".Board.BoardActivity" />
        <activity android:name=".Home.TypeActivity" />
        <activity android:name=".Home.VideoActivity" />
        <activity android:name=".MainActivity" />
        <activity
            android:name=".GetStartedMainActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>